<Window x:Name="SteamBetaConfirmation" x:Class="MotorTown_Dedicated_Server_Handler.AccessKeyPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MotorTown_Dedicated_Server_Handler"
        mc:Ignorable="d"
        Title="Steam Beta Code Needed!" Height="130" Width="479">
    <Grid>
        <Label Content="Please enter test branch code." HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="config_keyentry" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" Width="384"/>
        <Button x:Name="config_save_key_button" Content="Save" HorizontalAlignment="Center" Height="25" Margin="0,79,0,0" VerticalAlignment="Top" Width="61" Click="config_save_key_button_click"/>

    </Grid>
</Window>
